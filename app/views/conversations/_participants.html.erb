<% conversation.participants.each do |participant| %>
  <% unless participant == current_dreamer %>
    <%= link_to gravatar_tag(participant.email, class: "avatar", default: "http://img1.wikia.nocookie.net/__cb20111127192946/frontierville/images/f/f0/Shaman_Dream_Catcher-icon.png"), profile_path(participant) %>
  <% end %>
<% end %>