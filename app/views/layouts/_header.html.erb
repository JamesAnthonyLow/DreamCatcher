<header>
  <h1>DreamCatcher</h1>

  <nav>
    <% if session[:dreamer_id] %>
      <%= link_to "Profile", dreamer_path(session[:dreamer_id]) %>
      <%= link_to "Log Out", logout_path, method: "delete" %>
    <% else %>
      <%= link_to "Sign Up", new_dreamer_path %>
      <%= link_to "Log In", login_path %>
    <% end %>
  </nav>
</header>